@model IEnumerable<TravelPackage.Models.tpProductImages>

<div class="row">

    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <a href="@item.ImgPath" title="@item.AltName">
                <img width="100%;" src="@item.ImgPath" alt="@item.AltName">
            </a>
            <p>@item.Desc</p>
        </div>
    }

</div>


